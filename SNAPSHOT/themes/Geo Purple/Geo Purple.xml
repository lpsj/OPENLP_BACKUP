<?xml version="1.0" encoding="utf-8"?>
<theme version="2.0">
    <name>Geo Purple</name>
    <background type="image">
        <filename>geo5.jpg</filename>
        <borderColor>#000000</borderColor>
    </background>
    <font type="main">
        <name>PT Sans</name>
        <color>#FFFFFF</color>
        <size>80</size>
        <bold>True</bold>
        <italics>False</italics>
        <line_adjustment>0</line_adjustment>
        <location height="972" override="False" width="1900" x="10" y="0"/>
        <shadow shadowColor="#000000" shadowSize="2">True</shadow>
        <outline outlineColor="#000000" outlineSize="1">True</outline>
    </font>
    <font type="footer">
        <name>PT Sans Narrow</name>
        <color>#FFFFFF</color>
        <size>12</size>
        <bold>False</bold>
        <italics>False</italics>
        <line_adjustment>0</line_adjustment>
        <location height="108" override="False" width="1900" x="10" y="972"/>
        <shadow shadowColor="#000000" shadowSize="5">True</shadow>
        <outline outlineColor="#000000" outlineSize="2">False</outline>
    </font>
    <display>
        <horizontalAlign>2</horizontalAlign>
        <verticalAlign>1</verticalAlign>
        <slideTransition>False</slideTransition>
    </display>
</theme>
